{
  "name": "api-manager",
  "version": "0.1.0",
  "description": "CLI tool for API key and user management",
  "main": "dist/index.js",
  "bin": {
    "aimgr": "./bin/aimgr.js"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "start": "node bin/aimgr.js",
    "dev": "tsx src/index.ts",
    "dev:debug": "cross-env DEBUG=aimgr:* tsx src/index.ts",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,json,md}\" --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@commander-js/extra-typings": "^11.1.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "consola": "^3.4.0",
    "cspell": "^8.17.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "inquirer": "^12.4.1",
    "ofetch": "^1.4.1",
    "openai": "^4.83.0",
    "ora": "^8.2.0",
    "pnpm": "^10.2.1",
    "terminal-link": "^3.0.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^20.0.0",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.6.0",
    "oxlint": "^0.15.10",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.3.6",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
