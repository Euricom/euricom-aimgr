{
  "version": "0.2",
  "language": "en",
  "words": [
    "ofetch",
    "aimgr",
    "euricom",
    "openai",
    "openrouter",
    "anthropic",
    "pnpm",
    "tsup",
    "oxlint",
    "euri",
    "betterconsole",
    "picocolors",
    "tsnode"
  ],
  "ignorePaths": [
    "node_modules/**",
    "dist/**",
    "build/**",
    "coverage/**",
    ".git/**",
    "pnpm-lock.yaml",
    "*.log",
    "data/userdata.json",
    "rules/**"
  ],
  "allowCompoundWords": true,
  "ignoreRegExpList": ["\\(.*\\)", "\\[.*\\]"]
}
